
public class Solution {

    public static int linearSearch(int arr[], int x) {
    //  int[]  arr = [5,2,6,4,3,7]

    //  int t =9;
    //  int temp=0;
     for (int t = 0; t<arr.length; t++)
     {
       if(arr[t]==x){
         return t;
       }
      //  System.out.println("item is present at "+t+" index position)
      //  temp = temp+1;
     }
     return -1;

		//Your code goes here
    }
}
